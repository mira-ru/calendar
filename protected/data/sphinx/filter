index calendar_filter
{
	type=rt
	path			= /var/lib/sphinxsearch/data/calendar_filter
	rt_mem_limit		= 64M

	rt_field = name

	rt_attr_uint = type_id

	charset_type		= utf-8
	charset_table		= 0..9, A..Z->a..z, _, a..z, U+410..U+42F->U+430..U+44F, U+430..U+44F
	min_word_len		= 3

	#requare for word* search
	enable_star         = 1
	min_prefix_len	    = 3
	dict                = keywords
}