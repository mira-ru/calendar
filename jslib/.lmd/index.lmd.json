{
    "name": "Mock chat with LMD modules",
    
    "path": "../modules/",

    "output": "../index.lmd.js",

    "sourcemap": "../index.lmd.map",
    "sourcemap_inline": true,
    "sourcemap_www": "",

    "www_root": "../../../",

    "modules": {
        "main": "index.js",
        "$": {
            "path": "vendor/jquery.js",
            "exports": "jQuery"
        },
        "can": {
            "path": "vendor/can.js",
            "require": {
                "jQuery": "$"
            },
            "exports": "can"
        },
        "utils": {
            "path": "custom/common.js"
        }
    },
    "main": "main",
    "global": "this",
    "ie": false,
    "async": true,
    "lazy": false,
    "pack": false,
    "parallel": true,
    "shortcuts": false,

    "version": "1.0",

    "stats": false,
    "stats_coverage": false,
    "stats_coverage_async": false,
    "stats_sendto": false,
    "stats_auto": false,

    "warn": true,
    "log": true
}